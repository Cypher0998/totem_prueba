<odoo>

  <!-- treeview curso -->
  <record model="ir.ui.view" id="view_my_time_id">
    <field name="name">my_time.tree</field>
    <field name="model">my_time.my_time</field>
    <field name="priority" eval="16"/>
    <field name="arch" type="xml">
      <tree string="Horas disponibles">
        <field name="initial_hour"/>
        <field name="final_hour"/>
      </tree>
    </field>
  </record>

  <!-- formulario para ls imágenes -->
  <record model="ir.ui.view" id="view_id_my_time_form">
    <field name="name">my_time.form</field>
    <field name="model">my_time.my_time</field>
    <field name="priority" eval="16"/>
    <field name="arch" type="xml">
      <form string="Formulario de eventos">
        <sheet>
          <group>
            <field name="initial_hour"/>
            <field name="final_hour"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

<!-- vista de los cursos -->
  <record model="ir.actions.act_window" id="my_time_list_action">
    <field name="name">Vista de las Horas Disponibles</field>
    <field name="res_model">my_time.my_time</field>
    <field name="view_type">form</field>
    <field name="view_mode">tree,form</field>
  </record>

 <!--<menuitem id="time" name="Lista de imágenes" parent="main_menu" action="my_time_list_action"/> -->
</odoo>